{
  "loggerLevels" : {"DEBUG":"SYSOUT","INFO":"SYSOUT", "WARN":"SYSOUT", "ACCESS":"SYSOUT"},
  "staticPaths"  : {
    "windows":{"/static/":"site\\", "data":"site\\"}, 
    "linux":{"/static/":"site/", "data":"site/"},
    "darwin":{"/static/":"site/", "data":"site/"}
  }, 
  "templatePaths"  : {
    "windows":"templates\\", 
    "linux":"templates/",
    "darwin":"templates/"
  },
  "port" : 8080,
  "redirections" : {"/":"/static/index.html"},
  "contentTypes" : {"ico": "image/x-icon"},
  "contentTypeCharset":"utf-8",
  "panicResponseCode" : 500,
  "noResponseResponseCode" : 400,
  "templateData" : {
    "index1.html" : {"Material":"Soot"}
  },
  "scriptData" : {
    "windows" : {
      "path" : "site\\scripts\\",
      "data" : {
        "list" : ["cmd", "/C", "echop.bat", "${abc}-${xxx}"]
      }
    },
    "linux":{
      "path" : "site/scripts/",
      "data" : {
        "list" : ["sh", "-C", "echop.sh", "${abc}-${xxx}"]
      }
    },
    "darwin":{
      "path" : "site/scripts/",
      "data" : {
        "list" : ["sh", "-c", "echop.sh", "${abc}=${xxx}"]
      }
    }
  }
}
